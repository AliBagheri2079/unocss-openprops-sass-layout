{
  "name": "unocss-sass-rome-layout",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format:style": "pnpm run check:style --fix",
    "format:script": "rome format src/",
    "format:script:fix": "pnpm run format:script --write",
    "format": "pnpm run format:style && pnpm run format:script:fix",
    "check:style": "stylelint src/**/*.scss",

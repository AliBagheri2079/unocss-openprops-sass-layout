    "@types/react-dom": "^18.0.11",
    "@unocss/postcss": "^0.52.3",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^8.4.1",
    "postcss-scss": "^4.0.6",
    "rome": "^12.1.2",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-standard-scss": "^9.0.0",
    "unocss": "^0.52.3",
